        {
          "from": "step.1.content",
          "to": "trigger.0.content",
          "name": "content"
        },
        {
          "from": "step.1.fileNotFound",
          "to": "trigger.0.contentNotFound",
          "name": "fileNotFound"
        }
      ]
    },
    "triggers": []
  }
